{
    "manifest_version": 3,
    "name": "__MSG_extensionName__",
    "version": "1.1.1",

    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://github.com/kdrvs/SingleTab",

    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"

    },

    "permissions": [
        "activeTab",
        "storage",
        "scripting"  
    ],

    "host_permissions": ["*://*/*"],

    "web_accessible_resources": [{
        "resources": ["override.js"],
        "matches": ["<all_urls>"]
    }],
      

    "action": {
        "default_popup": "popup.html",
        "default_icon" : {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"

        }
    },

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["main.js"]
        }
    ],

    "default_locale": "en"
}